{
    "name": "musicversal-backend-challenge",
    "version": "1.0.0",
    "description": "musicversal-backend-challenge",
    "main": "src/index.js",
    "scripts": {
        "coverage": "nyc report --reporter=text-lcov | coveralls",
        "test": "yarn lint && yarn style:fix && yarn style:check && yarn test:unit && yarn test:functional",
        "lint": "eslint ./src ./test --ext .js",
        "lint:fix": "eslint ./src ./test --ext .js --fix",
        "test:functional": "dotenv -e .env -- jest --projects ./test --runInBand ---detectOpenHandles --forceExit",
        "test:unit": "dotenv -e .env -- jest",
        "start": "babel-node src/index.js",
        "start:dev": "nodemon --exec babel-node src/index.js | pino-pretty -c",
        "build": "babel src -d dist",
        "style:check": "prettier --check 'src/**/*.js' 'test/**/*.js'",
        "style:fix": "prettier --write 'src/**/*.js' 'test/**/*.js'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/markeu/musicversal-BE-Challenge"
    },
    "keywords": [
        "stock",
        "check_in",
        "check_out",
        "users"
    ],
    "author": "Uche Mark",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/markeu/musicversal-BE-Challenge/issues"
    },
    "homepage": "https://github.com/markeu/musicversal-BE-Challenge#readme",
    "dependencies": {
        "@babel/cli": "^7.12.8",
        "@babel/core": "^7.12.9",
        "@babel/node": "^7.12.6",
        "@babel/plugin-transform-runtime": "^7.17.0",
        "@babel/preset-env": "^7.12.7",
        "@babel/register": "^7.12.1",
        "@azure/msal-node": "^1.1.0",
        "@azure/storage-blob": "^12.8.0",
        "@babel/polyfill": "^7.12.1",
        "@babel/runtime": "^7.17.9",
        "chai": "^4.2.0",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-pino-logger": "^5.0.0",
        "express-rate-limit": "^5.1.3",
        "http-errors": "^1.8.1",
        "joi": "^17.4.2",
        "nodemon": "^2.0.15",
        "pino": "^6.3.2"
    },
    "devDependencies": {
        "chai-http": "^4.3.0",
        "coveralls": "^3.1.0",
        "dotenv-cli": "^3.2.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-import": "^2.22.1",
        "faker": "^5.1.0",
        "jest": "^26.6.3",
        "mocha": "^8.2.1",
        "mocha-lcov-reporter": "^1.3.0",
        "nyc": "^15.1.0",
        "pino-pretty": "^4.0.0",
        "prettier": "^2.0.5",
        "supertest": "^4.0.2"
    }
}